commit_count=$(shell git rev-list --count HEAD)
rev_id=$(shell git rev-parse --short HEAD)

migrate_version=0.1.$(commit_count)-$(rev_id)
upload_version=0.1.$(commit_count)-$(rev_id)
worker_version=0.2.$(commit_count)-$(rev_id)
status_version=0.1.$(commit_count)-$(rev_id)
result_version=0.1.$(commit_count)-$(rev_id)
inform_version=0.1.$(commit_count)-$(rev_id)
clean_version=0.1.$(commit_count)-$(rev_id)